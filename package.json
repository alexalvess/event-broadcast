{
  "name": "event-message-broker",
  "description": "A Message Bus that uses AWS Stack and RabbitMQ",
  "version": "1.0.15",
  "previousVersions": [],
  "main": "./build/cjs/index.js",
  "types": "./build/types/index.d.ts",
  "module": "./build/es/index.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build:clean": "rm -rf ./build",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:types": "tsc -p ./tsconfig.types.json",
    "build": "npm-run-all build:clean build:esm build:cjs build:types",
    "test": "jest --detectOpenHandles"
  },
  "author": {
    "name": "Alex Alves",
    "url": "https://github.com/alexalvess"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexalvess/event-broadcast"
  },
  "dependencies": {
    "@aws-sdk/client-scheduler": "^3.583.0",
    "@aws-sdk/client-sns": "^3.583.0",
    "@aws-sdk/client-sqs": "^3.583.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.47.1",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-jaeger": "^1.25.0",
    "@opentelemetry/exporter-prometheus": "^0.52.0",
    "@opentelemetry/instrumentation": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.52.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-metrics-base": "^0.31.0",
    "@opentelemetry/sdk-trace-node": "^1.25.0",
    "@opentelemetry/semantic-conventions": "^1.25.0",
    "@types/amqplib": "^0.10.5",
    "@types/uuid": "^9.0.8",
    "amqplib": "^0.10.4",
    "sqs-consumer": "^10.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "testcontainers": "^10.9.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 100000,
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "keywords": [
    "message bus",
    "bus",
    "message broker",
    "broker",
    "message",
    "event",
    "aws",
    "amazon",
    "sqs",
    "sns",
    "eventBridge",
    "rabbitmq"
  ]
}